<div class="container" id="kitchenTable">
  <h2>Kitchen Display</h2>
  <table class="table table-striped">
    <thead>
      <tr>
        <th class="product-id">ID</th>
        <th>Name</th>
        <th>Quantity</th>
        <th>Created Till Now</th>
        <th>Preidiction</th>
        <th>Status</th>
      </tr>
    </thead>
    <tbody>
      <% for(var i=0; i < products.length; i++) { %>
        <tr id="<%= products[i].id %>">
          <td class="product-id">
            <%= products[i].id %>
          </td>
          <td>
            <%= products[i].name %>
          </td>
          <td>
            <%= products[i].quantity %>
          </td>
          <td>
            <%= products[i].createdTillNow %>
          </td>
          <td>
            <%= products[i].predicted %>
          </td>
          <td>
            <button class="btn btn-sm doneButton btn-primary" value="<%= products[i].id %>">Done</button>
        </tr>
        <% } %>
    </tbody>
    <button id="reportButton" class="btn btn-sm btn-default" href="http://localhost:1337/createPdf" target="_blank">Download Report</button>
  </table>
</div>